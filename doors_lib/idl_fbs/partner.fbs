
include "base.fbs";
namespace partner;

table Partner{
    id: base.UByte16;//随机产生的一个唯一标识
    terminal_id: base.UByte16;//终端id
    partner_id: base.UByte16; //partner
    name: string;
    ip: string;
    port: int16;
    create_ts: base.Timestamp;
}

table OnLine{
    header: base.Header;
    partner: Partner;
    ts: base.Timestamp;
}

//确认收到消息
table OnLineAck{
    header: base.Header;
    //OnLine中的 id
    id: base.UByte16;
    partner: Partner;
    ts: base.Timestamp;
}